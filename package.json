    "@types/morgan": "^1.9.4",
    "axios": "^1.3.1",
    "express": "^4.18.2",
    "jest": "^29.4.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "socket.io": "^4.5.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "prettier": "^2.8.3"
  }
}
