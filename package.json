{
  "author": "Jacob Lurie",
  "license": "ISC",
  "name": "nightlight-backend",
  "version": "1.0.0",
  "description": "Backend for the nightlight mobile application Express, MongoDB, Mongoose, and Node.",
  "main": "src/server.ts",
  "scripts": {
    "start": "nodemon src/index.ts",
    "worker": "ts-node src/queue/setup/workers.setup.ts",
    "dev": "nodemon src/index.ts",
    "build": "rm -rf build/ && prettier --write src/ && tsc",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "seed": "nyc mocha --exit -r ts-node/register src/tests/seed.test.ts",
    "test-utils": "nyc mocha --exit -r ts-node/register src/tests/*.utils.test.ts",
    "test-venues": "nyc mocha --exit -r ts-node/register src/tests/venue.controller.test.ts",
    "test-users": "nyc mocha --exit -r ts-node/register src/tests/user.controller.test.ts",
    "test-groups": "nyc mocha --exit -r ts-node/register src/tests/group.controller.test.ts",
    "test": "nyc mocha --exit -r ts-node/register src/tests/*.test.ts"
  },
  "dependencies": {
    "@bull-board/express": "^5.0.0",
    "@faker-js/faker": "^7.6.0",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.16",
    "@types/mocha": "^10.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.4",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.12",
    "axios": "^1.3.1",
    "bull-arena": "^3.30.4",
    "bullmq": "^3.10.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fetch": "^1.1.0",
    "ioredis": "^5.3.1",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "socket.io": "^4.5.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  }
}
